{
  "name": "are-you-seeing-pictures",
  "version": "1.0.0",
  "homepage": "https://www.areyouseeing.pictures/",
  "private": false,
  "description": "are you seeing pictures",
  "main": "index.js",
  "scripts": {
    "web": "node index.js",
    "start": "concurrently --kill-others \"webpack serve --config webpack.development.js\"",
    "watch": "webpack --config webpack.development.js",
    "build": "webpack --config webpack.production.js",
    "build-dev": "webpack"
  },
  "engines": {
    "node": "17.1.0"
  },
  "author": "Sam Clarke",
  "license": "ISC",
  "bugs": "https://github.com/alruban/aysp-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/alruban/aysp-react"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@mugo-agency/eslint-config": "^0.0.5",
    "@mugo-agency/stylelint-config": "^0.0.9",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "buffer": "^6.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^5.1.13",
    "cssnano-preset-lite": "^2.1.3",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "os-browserify": "^0.3.0",
    "postcss": "^8.2.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-mixins": "^9.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.2",
    "postcss-pxtorem": "^6.0.0",
    "postcss-sassy-mixins": "^2.1.0",
    "postcss-simple-vars": "^7.0.0",
    "process": "^0.11.10",
    "react-hot-loader": "^4.13.0",
    "react-translate-json": "^2.1.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.7.3"
  },
  "postcss": {
    "plugins": [
      {
        "postcss-easy-import": {
          "extensions": ".pcss",
          "prefix": "_"
        }
      },
      {
        "postcss-inline-svg": {
          "paths": [
            "src/svgs"
          ]
        }
      },
      {
        "postcss-pxtorem": {
          "propList": [
            "font",
            "font-size",
            "line-height",
            "letter-spacing"
          ]
        }
      },
      "postcss-custom-media",
      "postcss-sassy-mixins",
      "postcss-nested",
      "postcss-simple-vars",
      "autoprefixer"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "concurrently": "^7.4.0",
    "minimist": "^1.2.0",
    "postcss-inline-svg": "^5.0.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-head": "^3.4.0",
    "react-helmet-async": "^1.2.3",
    "react-router-dom": "^6.2.2",
    "react-svg": "^14.1.13"
  }
}
